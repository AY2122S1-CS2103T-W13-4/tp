<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root minHeight="700" minWidth="1200.0" onCloseRequest="#handleExit" title="gitGud" type="javafx.stage.Stage"
         xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/gitGud.png"/>
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@Fonts.css"/>
                <URL value="@DarkTheme.css"/>
                <URL value="@Extensions.css"/>
            </stylesheets>

            <VBox prefHeight="215.0" prefWidth="600.0">
                <MenuBar fx:id="menuBar" prefWidth="615.0" VBox.vgrow="NEVER">
                    <Menu mnemonicParsing="false" text="File">
                        <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit"/>
                    </Menu>
                    <Menu mnemonicParsing="false" text="Help">
                        <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help"/>
                    </Menu>
                </MenuBar>
                <HBox prefHeight="479.0" prefWidth="594.0" VBox.vgrow="ALWAYS">
                    <children>
                        <VBox fx:id="friendBox" minWidth="111.0" prefHeight="180.0" prefWidth="111.0"
                              styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                            <padding>
                                <Insets bottom="10" left="10" right="10" top="10"/>
                            </padding>
                            <children>
                                <StackPane fx:id="friendsPlaceholder" prefHeight="300.0" prefWidth="140.0"
                                           VBox.vgrow="ALWAYS"/>
                            </children>
                        </VBox>
                        <VBox prefHeight="180.0" prefWidth="394.0" styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                            <children>
                                <StackPane fx:id="commandBoxPlaceholder" styleClass="pane-with-border"
                                           VBox.vgrow="NEVER">
                                    <padding>
                                        <Insets bottom="5" left="10" right="10" top="5"/>
                                    </padding>
                                </StackPane>
                                <StackPane fx:id="resultDisplayPlaceholder" maxHeight="111.0" minHeight="100.0"
                                           prefHeight="100.0" prefWidth="281.0" styleClass="pane-with-border"
                                           VBox.vgrow="NEVER">
                                    <padding>
                                        <Insets bottom="5" left="10" right="10" top="5"/>
                                    </padding>
                                </StackPane>
                                <HBox fx:id="mainCard" prefHeight="116.0" prefWidth="284.0"
                                      styleClass="pane-with-border" VBox.vgrow="ALWAYS">
                                    <children>
                                        <VBox fx:id="leftMainCard" prefHeight="112.0" prefWidth="144.0"
                                              HBox.hgrow="ALWAYS"/>
                                        <VBox fx:id="rightMainCard" maxHeight="1.7976931348623157E308"
                                              prefHeight="112.0" prefWidth="137.0" HBox.hgrow="ALWAYS">
                                            <HBox.margin>
                                                <Insets right="10.0"/>
                                            </HBox.margin>
                                        </VBox>
                                    </children>
                                    <padding>
                                        <Insets bottom="10.0" top="10.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                        <VBox fx:id="gameBox" minWidth="111.0" prefHeight="180.0" prefWidth="111.0"
                              styleClass="pane-with-border" HBox.hgrow="ALWAYS">
                            <padding>
                                <Insets bottom="10" left="10" right="10" top="10"/>
                            </padding>
                            <children>
                                <StackPane fx:id="gamesPlaceholder" prefHeight="300.0" prefWidth="140.0"
                                           VBox.vgrow="ALWAYS"/>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <StackPane fx:id="statusbarPlaceholder"/>
            </VBox>
        </Scene>
    </scene>
</fx:root>
